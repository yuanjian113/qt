		$.post("/index.php/report/select_report", { "id":"2792097" },function(data){
			if (data.succ == -1) {
				if(confirm('您还没登录!')){
					window.location.href = "http://passport.csdn.net/UserLogin.aspx?from=http://download.csdn.net%2Fdownload%2Fc3060911030%2F2792097";
					return false;
				}else{
					return false;
				}
			} else if(data.succ == 2) {
				$('.download_rep_suc').html('<span class="arrow"></span>资源已经被举报，无需重复举报');
				$('.download_rep_suc').slideDown('slow');
				setTimeout(function(){$('.download_rep_suc').slideUp('slow')},3000);
			} else {
				$("#pop_add_org").css("display","inline-block");
			}	
		}, "json");
	});
	
			
});

    // get cookie
    function get_cookie(name)
    {
        var search = name + '=';
        if(document.cookie.length > 0)
        {
            offset = document.cookie.indexOf(search);
            if(offset != 1)
            {
                offset += search.length;
                end = document.cookie.indexOf(';',offset);
                if(end == -1)
                {
                    end = document.cookie.length;
                }
                return unescape(document.cookie.substring(offset,end));
            }
            else
            {
                return '';
            }
        }
    }
</script>

<script type="text/javascript">

	function close_report(){
		$("#pop_add_org").css("display","none");
	}
	function submit_report(){
		
		var $report_type=	$("#report_type").val();
		var $report_description= $("#report_description").val();
		var num=0;

		if($report_type == '请选择类型'){
			num=num+1;
			$("#report_type_error").html("举报原因");
			return false;
		}else{
			$("#report_type_error").html('');

		}

		if($report_description == ''){
			num=num+1;
			$("#report_description_error").html("描述不能为空");
			return false;
		}else{
		 $("#report_description_error").html("");
			
		}
				if(num == 0){
				xhr = $('#reportform').ajaxSubmit({
					dataType: 'json',
					beforeSubmit: function(a,f,o) {
					
					},
					error:function(data,s,e)
					{
						alert(e);					
					},
					success: function(data) {
						if(data.succ==-1){
								alert("请先登录");
							}

						if(data.succ==1)
							{						
								alert("举报成功");			
								close_report();					
								return true;		
								}

						if(data.succ==2){						
								alert("已举报");
								}
										return false;
					}

				});
				}
		}

	$("#report_type").change(function(){
				if($(this).val() == "版权问题，侵犯个人或公司的版权"){
								$("#infringe").css("display","");
				}else{
								$("#infringe").css("display","none");
				}
		})

	</script>

